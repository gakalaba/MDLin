#!/bin/bash
python3 ./scripts/run_multiple_experiments.py ./experiments/osdi24/mp-9-3.json
wait
mv experiments/osdi24/results/tput-3/2* experiments/osdi24/results/tput-3/mp_batch_3ms_9s
rm -rf experiments/osdi24/results/tput-3/mp_batch_3ms_9s/2*/2*/out/client*
wait

python3 ./scripts/run_multiple_experiments.py ./experiments/osdi24/mp-9-4.json
wait
mv experiments/osdi24/results/tput-3/2* experiments/osdi24/results/tput-3/mp_batch_4ms_9s
rm -rf experiments/osdi24/results/tput-3/mp_batch_4ms_9s/2*/2*/out/client*
wait

python3 ./scripts/run_multiple_experiments.py ./experiments/osdi24/mp-9-5.json
wait
mv experiments/osdi24/results/tput-3/2* experiments/osdi24/results/tput-3/mp_batch_5ms_9s
rm -rf experiments/osdi24/results/tput-3/mp_batch_5ms_9s/2*/2*/out/client*
wait
